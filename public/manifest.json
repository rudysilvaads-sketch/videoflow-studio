{
  "manifest_version": 3,
  "name": "CharacterFlow - Personagens Consistentes para Veo 3",
  "description": "Crie vídeos com personagens consistentes no Google Flow/Veo 3. Gerencie prompts e use fallback automático.",
  "version": "1.0.0",
  "permissions": [
    "sidePanel",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://aitestkitchen.withgoogle.com/*",
    "https://labs.google/*"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "action": {
    "default_title": "Abrir CharacterFlow",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
